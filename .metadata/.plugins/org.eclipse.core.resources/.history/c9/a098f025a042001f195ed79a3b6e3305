package ap.services;

public interface Pharmacy {
	public List<PharmacyEO> getAllPharmacyDetails();
	public void addPharmacyDetails(PharmacyEO cusEORef);
	public void updatePharmacyDetails(PharmacyEO cusEORef);
	public void Optional<PharmacyEO> findByPharmacyId(String pharmacyId);
	public void deletePharmacyDetails(String pharmacyId);

}
